{
  "expo": {
    "name": "Bike Mesh",
    "slug": "bike-mesh",
    "description": "The best asset tracker app alive",
    "scheme": "bike-mesh",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/icons/ios-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icons/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F0F23"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cofounders.bikemesh",
      "infoPlist": {
        "NSBluetoothAlwaysUsageDescription": "This app uses Bluetooth to connect to bike tracking devices",
        "NSBluetoothPeripheralUsageDescription": "This app uses Bluetooth to connect to bike tracking devices",
        "NSLocationWhenInUseUsageDescription": "This app uses location to track your bike's position",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app uses location to track your bike's position in the background",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/android-icon.png",
        "backgroundColor": "#000"
      },
      "package": "com.cofounders.bikemesh",
      "permissions": [
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_ADMIN",
        "android.permission.BLUETOOTH_CONNECT",
        "android.permission.BLUETOOTH_SCAN",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION",
        "android.permission.CAMERA"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyC2npOce5wi7Zm5OMJKFYpRdA32y0XEjyk"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-build-properties",
      "expo-router",
      "expo-dev-client",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "recordAudioAndroid": false
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location to track your bike's position",
          "locationAlwaysPermission": "Allow $(PRODUCT_NAME) to use your location in the background to track your bike's position",
          "locationWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location to track your bike's position"
        }
      ],
      "react-native-ble-plx"
        ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f160dd12-9fc4-4319-b37b-1314561ae8f5"
      }
    },
    "runtimeVersion": "1.0.6",
    "updates": {
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/f160dd12-9fc4-4319-b37b-1314561ae8f5"
    },
    "owner": "ifedayoagboola19",
    "newArchEnabled": true
  }
}
